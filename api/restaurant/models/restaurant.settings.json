{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "name": "Restaurant",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "adress": {
      "type": "string",
      "required": true
    },
    "displayAddress": {
      "type": "string",
      "required": true
    },
    "restaurantName": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "bannerweb": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "bannerPhone": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "rating": {
      "type": "enumeration",
      "enum": [
        "one",
        "two",
        "three",
        "four",
        "five"
      ]
    },
    "status": {
      "type": "boolean",
      "default": false
    },
    "servingArea": {
      "type": "component",
      "repeatable": false,
      "component": "serving.serving-area"
    },
    "Commission": {
      "type": "component",
      "repeatable": false,
      "component": "commission.commission"
    }
  }
}
